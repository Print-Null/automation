{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "code": 1,
            "message": "success",
            "data": {
                "name": "保温杯",
                "mobile": "1351351350",
                "up_country_note": null,
                "timeout_at_text": "今天 17:49 ~ 19:49",
                "ticket_message": null
            }
        }
    ],
    "required": [
        "code",
        "message",
        "data"
    ],
    "additionalProperties": true,
    "properties": {
        "code": {
            "$id": "#/properties/code",
            "type": "integer",
            "title": "The code schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "message": {
            "$id": "#/properties/message",
            "type": "string",
            "title": "The message schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "success"
            ]
        },
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "The data schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "name": "保温杯",
                    "mobile": "1351351350",
                    "up_country_note": null,
                    "timeout_at_text": "今天 17:49 ~ 19:49",
                    "ticket_message": null
                }
            ],
            "required": [
                "name",
                "mobile",
                "up_country_note",
                "timeout_at_text",
                "ticket_message"
            ],
            "additionalProperties": true,
            "properties": {
                "name": {
                    "$id": "#/properties/data/properties/name",
                    "type": [
                        "string",
                        "null"
                    ],
                    "title": "The name schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "保温杯"
                    ]
                },
                "mobile": {
                    "$id": "#/properties/data/properties/mobile",
                    "type": "string",
                    "title": "The mobile schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "1351351350"
                    ]
                },
                "up_country_note": {
                    "$id": "#/properties/data/properties/up_country_note",
                    "type": "null",
                    "title": "The up_country_note schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": null,
                    "examples": [
                        null
                    ]
                },
                "timeout_at_text": {
                    "$id": "#/properties/data/properties/timeout_at_text",
                    "type": "string",
                    "title": "The timeout_at_text schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "今天 17:49 ~ 19:49"
                    ]
                },
                "ticket_message": {
                    "$id": "#/properties/data/properties/ticket_message",
                    "type": [
                        "null",
                        "string"
                    ],
                    "title": "The ticket_message schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": null,
                    "examples": [
                        null
                    ]
                }
            }
        }
    }
}