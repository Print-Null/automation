{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "code": 1,
            "message": "success",
            "data": {
                "prompt": true,
                "royalty_amount": 500
            }
        }
    ],
    "required": [
        "code",
        "message",
        "data"
    ],
    "properties": {
        "code": {
            "$id": "#/properties/code",
            "type": "integer",
            "title": "The code schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "message": {
            "$id": "#/properties/message",
            "type": "string",
            "title": "The message schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "success"
            ]
        },
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "The data schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "prompt": true,
                    "royalty_amount": 500
                }
            ],
            "required": [
                "prompt",
                "royalty_amount"
            ],
            "properties": {
                "prompt": {
                    "$id": "#/properties/data/properties/prompt",
                    "type": "boolean",
                    "title": "The prompt schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": false,
                    "examples": [
                        true
                    ]
                },
                "royalty_amount": {
                    "$id": "#/properties/data/properties/royalty_amount",
                    "type": "integer",
                    "title": "The royalty_amount schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        500
                    ]
                }
            },
            "additionalProperties": true
        }
    },
    "additionalProperties": true
}