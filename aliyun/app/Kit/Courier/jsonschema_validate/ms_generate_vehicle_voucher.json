{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "生成出车凭证",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "additionalProperties": true,
    "required": [
        "code",
        "message",
        "data"
    ],
    "properties": {
        "code": {
            "$id": "#/properties/code",
            "type": "integer",
            "title": "The Code Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1.0
            ]
        },
        "message": {
            "$id": "#/properties/message",
            "type": "string",
            "title": "The Message Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "success"
            ]
        },
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "The Data Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "id": "BKK3A3327",
                    "line_name": "曼谷应入仓2",
                    "plate_number": "2313(สมุทรปราการ)",
                    "existed": true
                }
            ],
            "additionalProperties": true,
            "required": [
                "id",
                "existed",
                "line_name",
                "plate_number"
            ],
            "properties": {
                "id": {
                    "$id": "#/properties/data/properties/id",
                    "type": "string",
                    "title": "The Id Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "BKK3A3327"
                    ]
                },
                "existed": {
                    "$id": "#/properties/data/properties/existed",
                    "type": "boolean",
                    "title": "The Existed Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": false,
                    "examples": [
                        true
                    ]
                },
                "line_name": {
                    "$id": "#/properties/data/properties/line_name",
                    "type": ["null","string"],
                    "title": "The Line_name Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "曼谷应入仓2"
                    ]
                },
                "plate_number": {
                    "$id": "#/properties/data/properties/plate_number",
                    "type": ["null","string"],
                    "title": "The Plate_number Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "2313(สมุทรปราการ)"
                    ]
                }
            }
        }
    }
}